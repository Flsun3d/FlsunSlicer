msgstr "ml"

#: libslic3r/PrintConfig.cpp:4135 libslic3r/PrintConfig.cpp:4136
msgid "Bottle weight"
msgstr "Peso botella"

#: libslic3r/PrintConfig.cpp:4137
msgid "kg"
msgstr "kg"

#: libslic3r/PrintConfig.cpp:4144
msgid "g/ml"
msgstr "g/ml"

#: libslic3r/PrintConfig.cpp:4151
msgid "money/bottle"
msgstr "dinero/botella"

#: libslic3r/PrintConfig.cpp:4156
msgid "Faded layers"
msgstr "Capas descoloridas"

#: libslic3r/PrintConfig.cpp:4157
msgid ""
"Number of the layers needed for the exposure time fade from initial exposure "
"time to the exposure time"
msgstr ""
"El número de capas necesarias para el tiempo de exposición cambie desde el "
"tiempo de exposición inicial hasta el tiempo de exposición"

#: libslic3r/PrintConfig.cpp:4164 libslic3r/PrintConfig.cpp:4165
msgid "Minimum exposure time"
msgstr "Tiempo de exposición mínimo"

#: libslic3r/PrintConfig.cpp:4172 libslic3r/PrintConfig.cpp:4173
msgid "Maximum exposure time"
msgstr "Tiempo de exposición máximo"

#: libslic3r/PrintConfig.cpp:4180 libslic3r/PrintConfig.cpp:4181
msgid "Exposure time"
msgstr "Tiempo de exposición"

#: libslic3r/PrintConfig.cpp:4187 libslic3r/PrintConfig.cpp:4188
msgid "Minimum initial exposure time"
msgstr "Tiempo de exposición inicial mínimo"

#: libslic3r/PrintConfig.cpp:4195 libslic3r/PrintConfig.cpp:4196
msgid "Maximum initial exposure time"
msgstr "Tiempo de exposición inicial máximo"

#: libslic3r/PrintConfig.cpp:4203 libslic3r/PrintConfig.cpp:4204
msgid "Initial exposure time"
msgstr "Tiempo de exposición inicial"

#: libslic3r/PrintConfig.cpp:4210 libslic3r/PrintConfig.cpp:4211
msgid "Correction for expansion"
msgstr "Corrección para la expansión"

